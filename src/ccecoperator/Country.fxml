<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ccecoperator.CountryController">
  <children>
    <Button layoutX="18.0" layoutY="96.0" mnemonicParsing="false" onAction="#handleButtonBackAction" text="Back" />
    <Button layoutX="18.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleButtonCountriesAction" text="ViewCountry" />
    <TableView fx:id="countryTable" layoutX="132.0" layoutY="96.0" prefHeight="248.0" prefWidth="417.0">
      <columns>
        <TableColumn prefWidth="75.0" text="ID" fx:id="idCountry" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="114.0" text="Name" fx:id="countryName" />
        <TableColumn prefWidth="75.0" text="Currency" fx:id="currency" />
        <TableColumn prefWidth="75.0" text="Language" fx:id="language" />
        <TableColumn prefWidth="75.0" text="Visa Charge" fx:id="visaCharge" />
      </columns>
    </TableView>
  </children>
</AnchorPane>
